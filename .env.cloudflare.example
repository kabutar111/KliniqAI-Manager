# Cloudflare + R2 configuration (example)

# Account and API
CF_ACCOUNT_ID=your_account_id
CF_API_TOKEN=cloudflare_api_token_with_workers_r2_permissions

# R2 (S3-compatible)
R2_ACCESS_KEY_ID=your_r2_access_key
R2_SECRET_ACCESS_KEY=your_r2_secret_key
R2_BUCKET=kliniqai-content
# Optional override; defaults to https://${CF_ACCOUNT_ID}.r2.cloudflarestorage.com
R2_ENDPOINT=

# Worker runtime vars
INTERNAL_BETA=true
BETA_KEY=dev-beta-key-rotate-me
CORS_ORIGIN=*

# Domains (examples)
CONTENT_ROUTE_DOMAIN=content-staging.kliniqai.com
CONTENT_ZONE_NAME=kliniqai.com
